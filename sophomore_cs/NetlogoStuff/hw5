#reader(lib "htdp-beginner-reader.ss" "lang")((modname hw5) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))
(define HarMean3
  (lambda (a b c)
    (/ 1 (/ (+ (/ 1 a) (/ 1 b) (/ 1 c)) 3))
       )
    )
  (HarMean3 5 -10 20)